{
    "name": "YunoRunner",
    "id": "yunorunner",
    "packaging_format": 1,
    "description": {
        "en": "CI runner of YunoHost",
        "fr": "Runner d'intÃ©gration continue de YunoHost"
    },
    "version": "2022.01.18~ynh3",
    "url": "https://github.com/YunoHost/yunorunner",
    "upstream": {
        "license": "GPL-3.0-or-later",
        "code": "https://github.com/YunoHost/yunorunner"
    },
    "license": "GPL-3.0-or-later",
    "maintainer": {
        "name": ""
    },
    "previous_maintainer": {
        "name": "Maniack Crudelis",
        "email": "maniackc_dev@crudelis.fr"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/yunorunner",
                "default": "/yunorunner"
            },
            {
                "name": "ci_package_check_path",
                "type": "string",
                "optional": "True",
                "example": "/home/CI_package_check",
                "default": "/home/CI_package_check",
                "ask": {
                    "en": "Provide the CI_package_check path"
                }
            },
            {
                "name": "ci_package_check_datadir",
                "type": "string",
                "optional": "True",
                "example": "/home/CI_package_check",
                "default": "/home/CI_package_check",
                "ask": {
                    "en": "Provide the CI_package_check datadir"
                }
            }
        ]
    }
}
